% Display of data
disp('Data matrix (data):');
disp(data);
disp('Result vector (target):');
disp(target);
% Completion of the column of ones to the data matrix 
X = [ones(size(data, 1), 1), data];

% Calculation of regression coefficients
beta = (X' * X) \ (X' * target);

% Prediction of values based on the model
yPred = X * beta;

% Regression coefficients
disp('Regression coefficients');
disp(beta);
% Number of trials (n) and number of variables (p)
n = size(data, 1);
p = size(X, 2);

% Stopnie swobody (degrees of freedom)
df_total = n - 1;
df_regression = p - 1;
df_residual = n - p;

% Degrees of freedom
ss_total = sum((target - mean(target)).^2);
ss_regression = sum((yPred - mean(target)).^2);
ss_residual = sum((target - yPred).^2);

% Calculation of MSE and RMSE
mse = ss_residual / df_residual;
rmse = sqrt(mse);

% Display of results
disp(['Degrees of freedom (total):', num2str(df_total)]);
disp(['Degrees of freedom (regression):', num2str(df_regression)]);
disp(['Degrees of freedom (residua):', num2str(df_residual)]);
disp(['Sum of squares (total):', num2str(ss_total)]);
disp(['Sum of squares (regression):', num2str(ss_regression)]);
disp(['Sum of squares (residua): ', num2str(ss_residual)]);
disp(['Mean square error (MSE): ', num2str(mse)]);
disp(['Root Mean Square Error (RMSE):', num2str(rmse)]);
% % Visualisation of actual and projected values
figure;
scatter(target, yPred, 'b', 'filled');
hold on;
plot([min(target), max(target)], [min(target), max(target)], 'r--', 'LineWidth', 2);
legend('Predictable', 'Perfect fit');
title('Real vs Predicted values of carrot root yield losses');
xlabel('Real yield losses [%]');
ylabel('Prediction of yield loss [%]');
grid on;
hold off;

% Graph of residuals
residuals = target - yPred;
figure;
scatter(yPred, residuals, 'b', 'filled');
hold on;
plot([min(yPred), max(yPred)], [0, 0], 'r--', 'LineWidth', 2);
legend('Residuals', 'Zero line');
title('Distribution of residuals in relation to predicted yield loss values');
xlabel('Predicting yield losses');
ylabel('Rests');
grid on;
hold off;
% Calculation of the coefficient of determination (R^2)
r2 = 1 - (ss_residual / ss_total);

% % Display R^2
disp(['Coefficient of determination (R^2): ', num2str(r2)]);
